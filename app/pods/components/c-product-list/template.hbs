<div class="list-page">
  <ul class="list-page__content product-list">
    {{#each products as |product|}}
    <li class="list-page__item">
      {{c-product-tile
        title=product.title
        image=product.image
        description=product.description
        rating=product.rating
        price=product.price
        unit=product.unit
        currency=product.currency
        isDiscounted=product.isDiscounted
      }}
    </li>
    {{/each}}
  </ul>
</div>

{{yield}}