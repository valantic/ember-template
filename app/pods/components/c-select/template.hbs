{{#e-panel
  layout=layout
  state=state
}}
  {{#if label.length}}
    {{e-label
      class=labelClassName
      for=fieldId
      layout=layout
      required=required
      state=state
      text=label
      multiple=multiple
      leading=true
    }}
  {{/if}}
  {{e-select
    class=fieldClassName
    disabled=disabled
    groupValue=groupValue
    id=fieldId
    layout=layout
    name=name
    multiple=multiple
    placeholder=placeholder
    readonly=readonly
    items=items
    size=size
    state=state
    title=label
    dataTextField=(if dataTextField dataTextField "label")
    dataValueField=(if dataValueField dataValueField "value")
  }}
{{/e-panel}}
