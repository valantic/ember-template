{{#if title}}
<h2 class="{{classNames.[1]}}__title">{{title}}</h2>
{{/if}}
<ul class="{{classNames.[1]}}__menu">
  {{#each nav.links as |link|}}
  <li class="{{classNames.[1]}}__menu-item">
    {{#link-to link.url}}{{link.label}}{{/link-to}}
    {{#if link.children.length}}
    <ul class="{{classNames.[1]}}__menu">
    {{#each link.children as |child|}}
      <li class="{{classNames.[1]}}__menu-item">{{#link-to child.url}}{{child.label}}{{/link-to}}</li>
    {{/each}}
    </ul>
    {{/if}}
  </li>
  {{/each}}
</ul>
{{yield}}