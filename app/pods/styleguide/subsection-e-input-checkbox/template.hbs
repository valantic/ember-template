          name="checkbox10"
          state="success"
          checked=true
        }}
      {{/freestyle-usage}}
    {{/collection.variant}}

    {{#collection.variant key='warning'}}
      {{#freestyle-usage 'e-input-checkbox-state-warning' title='e-input-checkbox warning' usageTitle=labels.usage.title}}
