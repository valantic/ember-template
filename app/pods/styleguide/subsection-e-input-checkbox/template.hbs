{{#section.subsection name='e-input-checkbox'}}
  <div class="FreestyleAnnotation">
    <p>This elements are here only visible, they aren't clickable because they reach there functionality in the c-input-radio component together with a "fake-label"</p>
    <a href="styleguide?s=Components&ss=c-input-checkbox">c-input-checkbox</a>
  </div>
  {{#freestyle-annotation}}
    <p>Extends: <em>eFormComponent</em></p>
    <h4>Attributes</h4>
    <dl>
      <dt><code>disabled</code> (Boolean)</dt>
      <dd>sets element-state to disabled</dd>
      <dt><code>checked</code> (Boolean)</dt>
      <dd>state of the checkbox</dd>
      <dt><code>required</code> (Boolean)</dt>
      <dd>required attribute of element</dd>
      <dt><code>autofocus</code> (Boolean)</dt>
      <dd>sets autofocus attribute to element</dd>
      <dt><code>name</code> (String)</dt>
      <dd>name attribute of element</dd>
      <dt><code>id</code> (String)</dt>
      <dd>id attribute of element</dd>
    </dl>
  {{/freestyle-annotation}}

  {{#freestyle-collection title='e-input-checkbox' defaultKey='all' inline=false as |collection|}}

    {{#collection.variant key='default'}}
      {{#freestyle-usage 'e-input-checkbox-default' title='e-input-checkbox default' usageTitle=labels.usage.title}}
        {{e-input-checkbox
          name="checkbox1"
        }}
      {{/freestyle-usage}}
    {{/collection.variant}}

    {{#collection.variant key='checked'}}
      {{#freestyle-usage 'e-input-checkbox-checked' title='e-input-checkbox checked' usageTitle=labels.usage.title}}
        {{e-input-checkbox
          name="checkbox2"
          checked=true
        }}
      {{/freestyle-usage}}
    {{/collection.variant}}

    {{#collection.variant key='with class name'}}
      {{#freestyle-usage 'e-input-checkbox-class' title='e-input-checkbox class' usageTitle=labels.usage.title}}
        {{e-input-checkbox
          name="checkbox3"
          class="ExampleClassCheckbox"
        }}
      {{/freestyle-usage}}
    {{/collection.variant}}

  {{/freestyle-collection}}



  {{#freestyle-collection title='e-input-checkbox states' defaultKey='all' inline=false as |collection|}}

    {{#collection.variant key='default'}}
      {{#freestyle-usage 'e-input-checkbox-state-default' title='e-input-checkbox default' usageTitle=labels.usage.title}}
        {{e-input-checkbox
          state="default"
          checked=true
        }}
      {{/freestyle-usage}}
    {{/collection.variant}}

    {{#collection.variant key='success'}}
      {{#freestyle-usage 'e-input-checkbox-state-success' title='e-input-checkbox success' usageTitle=labels.usage.title}}
