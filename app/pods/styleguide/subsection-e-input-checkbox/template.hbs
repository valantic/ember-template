          name="checkbox11"
          state="warning"
          checked=true
        }}
      {{/freestyle-usage}}
    {{/collection.variant}}

    {{#collection.variant key='error'}}
      {{#freestyle-usage 'e-input-checkbox-state-error' title='e-input-checkbox error' usageTitle=labels.usage.title}}
