<h2>Order List</h2>
<table>
  <thead>
    <td>ID</td>
    <td>Order Number</td>
    <td>Created at</td>
    <td>Amount of items</td>
    <td>Status code</td>
  </thead>
  <tbody>
  {{#each model as |order|}}
    <tr>
      <td>{{order.id}}</td>
      <td>{{order.orderNumber}}</td>
      <td>{{order.createdAt}}</td>
      <td>{{order.amountOfLineItems}}</td>
      <td>{{order.statusCode}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>
{{outlet}}
